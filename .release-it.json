{
  "git": {
    "commitMessage": "chore: release ${version}"
  },
  "github": {
    "release": true
  },
  "npm": {
    "publish": false
  },
  "plugins": {
    "@release-it/conventional-changelog": {
      "preset": {
        "name": "conventionalcommits"
      },
      "ignoreRecommendedBump": false
    },
    "@release-it/bumper": {
      "out": ["dotmix/__init__.py", "dotmix/cli/__init__.py"]
    }
  }
}
